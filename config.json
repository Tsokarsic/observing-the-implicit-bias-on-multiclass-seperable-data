{
    "experiment_name": "ImplicitBias_MaxMargin_Analysis",
    "wandb_project": "Multiclass_Implicit_Bias",

    "data": {
        "data_path": "./experiment_data/data_with_highdim_noise.json",
        "regenerate_if_not_found": true,
        "save_dir": "./experiment_data/",
        "k": 10,
        "d": 25,
        "n_per_class": 50,
        "sigma": 0.1,
        "seed": 45
    },

    "training": {
        "epochs": 10000,
        "optimizer": "GD",
        "log_interval": 100,
        "lr_schedule": {
        "type": "sqrt_t_decay_with_warmup",
        "warmup_steps": 500,
        "t0": 100.0
        },

        "optimizer_params": {
            "GD": {"lr": 0.1, "momentum": 0.0},
            "Adam": {"lr": 0.001, "betas": [0.9, 0.999], "eps": 1e-8, "weight_decay": 0},
            "SignGD": {"lr": 0.001, "eps": 1e-8}
        }
    }
}